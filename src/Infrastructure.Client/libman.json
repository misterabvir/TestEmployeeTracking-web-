{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.11.2",
      "destination": "wwwroot/css/bootstrap-icons/"
    },
    {
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/libs/bootstrap/"
    },
    {
      "provider": "unpkg",
      "library": "fontawesome@5.6.3",
      "destination": "wwwroot/libs/fontawesome/"
    },
    {
      "provider": "unpkg",
      "library": "@fortawesome/free-solid-svg-icons@6.5.1",
      "destination": "wwwroot/libs/fortawesome/free-solid-svg-icons/"
    }
  ]
}